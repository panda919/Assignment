{
  "name": "Assignment",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/Assignment/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android": "react-native run-android",
    "ipa": "react-native run-ios  --configuration=release",
    "ios": "react-native run-ios --simulator='iPhone 8 Plus'",
    "device": "react-native run-ios --device",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "immutability-helper": "^3.0.0",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.22.2",
    "native-base": "^2.8.1",
    "prop-types": "^15.6.2",
    "react": "16.6.1",
    "react-native": "0.57.7",
    "react-native-elements": "^1.1.0",
    "react-native-gesture-handler": "^1.0.10",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-tts": "^3.0.0",
    "react-native-vector-icons": "^6.1.0",
    "react-native-voice": "^0.2.6",
    "react-navigation": "^3.0.5",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-axios-middleware": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-jest": "24.8.0",
    "jest": "24.8.0",
    "metro-react-native-babel-preset": "0.54.0",
    "react-test-renderer": "16.6.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
